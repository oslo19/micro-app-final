{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js",
      "headers": {
        "cache-control": "s-maxage=3600"
      }
    }
  ],
  "functions": {
    "server.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
} 
